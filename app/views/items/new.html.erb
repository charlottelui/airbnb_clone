<%= simple_form_for [@item] do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :photos, as: :file, input_html: { multiple: true } %>
      <%= f.button :submit, class: 'button' %>
    <% end %>

<%= link_to "homepage", items_path %>
